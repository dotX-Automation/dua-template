  // Use Pylance as language server
  "python.languageServer": "Pylance",
  "python.analysis.extraPaths": [
    "/opt/dua-venv/lib/python3.10/site-packages/",
    "/opt/ros/humble/lib/python3.10/site-packages/",
    "/opt/ros/humble/local/lib/python3.10/dist-packages/",
    "/opt/ros/dua-utils/install/lib/python3.10/site-packages/",
    "/opt/ros/dua-utils/install/local/lib/python3.10/dist-packages/"
    // Add your own paths here!
  ],
  // Autocomplete from ros python packages
  "python.autoComplete.extraPaths": [
    "/opt/dua-venv/lib/python3.10/site-packages/",
    "/opt/ros/humble/lib/python3.10/site-packages/",
    "/opt/ros/humble/local/lib/python3.10/dist-packages/",
    "/opt/ros/dua-utils/install/lib/python3.10/site-packages/",
    "/opt/ros/dua-utils/install/local/lib/python3.10/dist-packages/"
    // Add your own paths here!
  ],
  // Environment file lets vscode find python files within workspace
  "python.envFile": "${workspaceFolder}/.env",
  // Use autopep8 for Python formatting
  "[python]": {
      "editor.defaultFormatter": "ms-python.autopep8"
  },
  "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
  "C_Cpp.formatting": "disabled",
  "uncrustify.configPath.linux": "/opt/ros/humble/lib/python3.10/site-packages/ament_uncrustify/configuration/ament_code_style.cfg",
  "[cpp]": {
    "editor.defaultFormatter": "zachflower.uncrustify"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/build": true,
    "**/install": true,
    "**/log": true
  },
  "shellcheck.enable": true,
  "shellcheck.enableQuickFix": true,
  "shellcheck.run": "onType",
  "shellcheck.executablePath": "/usr/bin/shellcheck",
  "shellcheck.exclude": [
    "1091"
  ],
  "shellcheck.customArgs": [],
  "shellcheck.ignorePatterns": {
    "**/*.xonshrc": true,
    "**/*.xsh": true,
    "**/*.zsh": true,
    "**/*.zshrc": true,
    "**/zshrc": true,
    "**/*.zprofile": true,
    "**/zprofile": true,
    "**/*.zlogin": true,
    "**/zlogin": true,
    "**/*.zlogout": true,
    "**/zlogout": true,
    "**/*.zshenv": true,
    "**/zshenv": true,
    "**/*.zsh-theme": true
  },
  "ros.distro": "humble"
}